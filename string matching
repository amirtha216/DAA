#include <stdio.h>
#include <string.h>

int main() {
    char text[] = "example";
    char pattern[] = "am";
    int n = strlen(text);
    int m = strlen(pattern);
    int matchCount = 0;

    for (int i = 0; i <= n - m; i++) {
        int j;
        for (j = 0; j < m; j++) {
            if (text[i + j] != pattern[j]) {
                break;
            }
        }
        if (j == m) {
            matchCount++;
        }
    }

    printf("Number of matches found: %d\n", matchCount);

    return 0;
}
